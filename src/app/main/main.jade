.container#main
  .section
    h1 Lo mejor de Bonappettit
    img(ng-if="!main.data" src="assets/images/loading.gif")
    .row
      .col-sm-6.col-md-4(ng-repeat="d in main.data.slice(0,6)")
        .thumbnail
          a(ui-sref="dishInfo({id: d.id})" ng-click="main.clickDish(d.id)"): img(data-holder-rendered="true" src="data:image/svg+xml;base64," style="display: block;" alt="")
        uib-rating.rating(ng-model="d.averageRating" max="5" read-only="true" on-leave="overStar = null" titles="['one','two','three']" aria-labelledby="default-rating")
        .caption
          a.color_dish_name(ui-sref="dishInfo({id: d.id})"): h3 {{d.name}}
          a.color_dish_description(ui-sref="dishInfo({id: d.id})"): p {{d.description}}
  .section
    h1 Recomendado para ti
    img(ng-if="!main.data" src="assets/images/loading.gif")
    .row
      .col-sm-6.col-md-4(ng-repeat="d in main.data.slice(6,12)")
        .thumbnail
          a(ui-sref="dishInfo({id: d.id})" ng-click="main.clickDish(d.id)"): img(data-holder-rendered="true" src="data:image/svg+xml;base64," style="display: block;" alt="")
        uib-rating.rating(ng-model="d.averageRating" max="5" read-only="true" on-leave="overStar = null" titles="['one','two','three']" aria-labelledby="default-rating")
        .caption
          a.color_dish_name(ui-sref="dishInfo({id: d.id})"): h3 {{d.name}}
          a.color_dish_description(ui-sref="dishInfo({id: d.id})"): p {{d.description}}
  br
