.container
  .row.separation_profile
    .col-xs-12
      .col-xs-2#profile-pic
        img(src='data:image/svg+xml;base64,', alt='...')
      .col-xs-10
        h2.text_name_user  {{profile.user.name + ' ' + profile.user.lastname}}
        p.text_profile #[b Fecha de nacimiento]: {{profile.user.birthdate}}
        p.text_profile #[b País: ] {{profile.user.country}}
        p.text_profile #[b Miembro desde]: {{profile.user.since}}
    //button.btn.btn-success.pull-right(ng-if="profile.editable") Editar tu información &nbsp;
      span.glyphicon.glyphicon-pencil

.section.col-xs-11.my_dishes.separation_dishes
  .row
    h3.my_dishes_text Mis platillos
    button.pull-right.btn.btn-success.btn-plus(ng-if="profile.editable" ui-sref="createDish") Añadir nuevo platillo &nbsp;
      span.glyphicon.glyphicon-plus
    br
  .row
    .col-xs-6.col-md-3.section(ng-repeat="i in profile.dishes")
      a.thumbnail(href='')
        img(src='data:image/svg+xml;base64,', alt='...')
        p.dish_name.text-center.my_dishes Platillo {{i}}
      uib-rating.rating(ng-model="i.averageRating" max="5" read-only="true" on-leave="overStar = null" titles="['one','two','three']" aria-labelledby="default-rating")
      .over
        button.btn.btn-danger.pull-right(ng-click="profile.deleteDish(i)")
          span.glyphicon.glyphicon-trash
        button.btn.btn-primary.pull-right(ng-click="profile.editDish(i)")
          span.glyphicon.glyphicon-pencil
      
  .row
    h4.my_dishes_text(ng-if="profile.dishes.length === 0") No se ha encontrado ningún platillo
